{
  "name": "react-state-warp",
  "version": "0.0.1",
  "description": "Teleport React state between devices instantly using QR codes & WebRTC.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run --reporter=verbose",
    "test:watch": "vitest",
    "lint": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "dependencies": {
    "peerjs": "^1.5.0",
    "uuid": "^11.0.0"
  },
  "devDependencies": {
    "@testing-library/react": "^16.3.1",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "global-jsdom": "^27.0.0",
    "jsdom": "^27.4.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^4.0.16"
  },
  "keywords": [
    "react",
    "state",
    "webrtc",
    "sync",
    "p2p",
    "hook"
  ],
  "author": "Reza Kheradmandi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Reza-kh80/react-state-warp.git"
  },
  "bugs": {
    "url": "https://github.com/Reza-kh80/react-state-warp/issues"
  },
  "homepage": "https://github.com/Reza-kh80/react-state-warp#readme"
}